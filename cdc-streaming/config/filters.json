{
  "version": 1,
  "filters": [
    {
      "id": "car-created-filter",
      "name": "Car Created Events",
      "description": "Filters CarCreated events by event_type",
      "consumerId": "car-consumer",
      "outputTopic": "filtered-car-created-events",
      "enabled": true,
      "conditions": [
        {
          "field": "event_type",
          "operator": "equals",
          "value": "CarCreated",
          "valueType": "string"
        },
        {
          "field": "__op",
          "operator": "equals",
          "value": "c",
          "valueType": "string"
        }
      ],
      "conditionLogic": "AND",
      "status": "deployed",
      "version": 1
    },
